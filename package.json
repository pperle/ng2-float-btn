{
  "name": "ng2-float-btn",
  "version": "0.0.4",
  "description": "Button group for angular 2 and angular material 2.",
  "keywords": [
    "angular 2",
    "angular material 2",
    "fab",
    "button group",
    "collapse"
  ],
  "homepage": "https://github.com/BaronChen/ng2-float-btn",
  "bugs": {
    "url": "https://github.com/BaronChen/ng2-float-btn/issues"
  },
  "author": "baron chen, Xiaolong Tang",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/BaronChen/ng2-float-btn.git"
  },
  "main": "dist/ng2-float-btn.umd.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "webpack-dev-server --quiet --port 9002 --content-base app --config app/webpack.config --open",
    "test": "karma start",
    "lint": "tslint \"src/**/*.ts\" --project tsconfig.json --type-check",
    "clear": "rimraf dist",
    "build": "npm-run-all --serial clear build:ngc build:umd copy",
    "build:ngc": "ngc -p tsconfig.ngc.json",
    "build:umd": "cross-env NODE_ENV=production webpack",
    "build:app": "cross-env webpack --config app/webpack.config",
    "copy": "copyfiles README.md dist && copyfiles -u 1 ./build-helper/package.json dist"
  },
  "dependencies": {},
  "devDependencies": {
    "@angular/cdk": "^2.0.0-beta.12",
    "@angular/common": "^4.4.4",
    "@angular/compiler": "^4.4.4",
    "@angular/compiler-cli": "^4.4.4",
    "@angular/core": "^4.4.4",
    "@angular/forms": "^4.4.4",
    "@angular/http": "^4.4.4",
    "@angular/material": "^2.0.0-beta.12",
    "@angular/platform-browser": "^4.4.4",
    "@angular/platform-browser-dynamic": "^4.4.4",
    "@angular/router": "^4.4.4",
    "angular2-template-loader": "^0.6.2",
    "codelyzer": "^2.1.1",
    "cross-env": "^3.2.4",
    "jasmine-core": "2.5.2",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript": "^2.1.7",
    "npm-run-all": "^4.1.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "rxjs": "^5.5.2",
    "source-map-loader": "^0.1.6",
    "strip-loader": "^0.1.2",
    "systemjs": "0.19.40",
    "ts-helpers": "^1.1.2",
    "ts-loader": "^2.0.0",
    "ts-node": "3.0.4",
    "tslint": "^4.0.0",
    "typescript": "^2.6.2",
    "webpack": "^2.4.0",
    "webpack-dev-server": "^2.4.5",
    "zone.js": "^0.8.4",
    "core-js": "^2.4.1",
    "copyfiles": "^1.2.0"      
  }
}
